

#include<stdio.h>

int main()
{
    //Loop through an array and check if the entered number is in the list. Display output messages for each check.
    
    int numbers[]={100,200,300,650};
    int i,guess;
    printf("Take a guess \n");
    scanf("%d", &guess);
    
    for (i=0; i<4; i++)
    {
        if (guess==numbers[i])
        {
            printf("Correct\n");
        }
           else  printf("Missed\n");
    }

    return 0;
}
